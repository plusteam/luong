<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:orientation="vertical" android:layout_width="match_parent"
    android:layout_height="match_parent">

   <ScrollView
       android:layout_width="wrap_content"
       android:layout_height="wrap_content">
       <TableLayout
           android:layout_width="wrap_content"
           android:layout_height="wrap_content"
           android:orientation="vertical">
           <TableRow
               android:layout_marginTop="10dp">
               <TextView
                   android:layout_width="wrap_content"
                   android:layout_height="wrap_content"
                   android:text="Company"
                   android:textSize="17dp"
                   android:layout_marginLeft="10dp"/>
               <TextView
                   android:id="@+id/vCompany"
                   android:layout_width="270dp"
                   android:layout_height="wrap_content"
                   android:background="@drawable/vienvuong"
                   android:layout_marginLeft="10dp"/>
           </TableRow>
           <TableRow
               android:layout_marginTop="10dp">
               <TextView
                   android:layout_width="wrap_content"
                   android:layout_height="wrap_content"
                   android:text="Type"
                   android:textSize="17dp"
                   android:layout_marginLeft="10dp"/>
               <TextView
                   android:id="@+id/vType"
                   android:layout_width="270dp"
                   android:layout_height="wrap_content"
                   android:background="@drawable/vienvuong"
                   android:layout_marginLeft="10dp"/>
           </TableRow>
           <TableRow
               android:layout_marginTop="10dp">
               <TextView
                   android:layout_width="wrap_content"
                   android:layout_height="wrap_content"
                   android:text="Rate"
                   android:textSize="17dp"
                   android:layout_marginLeft="10dp"/>
               <TextView
                   android:id="@+id/vRate"
                   android:layout_width="270dp"
                   android:layout_height="wrap_content"
                   android:background="@drawable/vienvuong"
                   android:layout_marginLeft="10dp"/>
           </TableRow>
           <TableRow
               android:layout_marginTop="10dp">
               <TextView
                   android:layout_width="wrap_content"
                   android:layout_height="wrap_content"
                   android:text="Address"
                   android:textSize="17dp"
                   android:layout_marginLeft="10dp"/>
               <TextView
                   android:id="@+id/vAddress"
                   android:layout_width="270dp"
                   android:layout_height="wrap_content"
                   android:background="@drawable/vienvuong"
                   android:layout_marginLeft="10dp"/>
           </TableRow>
           <TableRow
               android:layout_marginTop="10dp">
               <TextView
                   android:layout_width="wrap_content"
                   android:layout_height="wrap_content"
                   android:text="Project"
                   android:textSize="17dp"
                   android:layout_marginLeft="10dp"/>
               <TextView
                   android:id="@+id/vProject"
                   android:layout_width="270dp"
                   android:layout_height="wrap_content"
                   android:background="@drawable/vienvuong"
                   android:layout_marginLeft="10dp"/>
           </TableRow>
           <TableRow
               android:layout_marginTop="10dp">
               <TextView
                   android:layout_width="wrap_content"
                   android:layout_height="wrap_content"
                   android:text="Email"
                   android:textSize="17dp"
                   android:layout_marginLeft="10dp"/>
               <TextView
                   android:id="@+id/vEmail"
                   android:layout_width="270dp"
                   android:layout_height="wrap_content"
                   android:background="@drawable/vienvuong"
                   android:layout_marginLeft="10dp"/>
           </TableRow>
           <TableRow
               android:layout_marginTop="10dp">
               <TextView
                   android:layout_width="wrap_content"
                   android:layout_height="wrap_content"
                   android:text="Note"
                   android:textSize="17dp"
                   android:layout_marginLeft="10dp"/>
               <TextView
                   android:id="@+id/vNote"
                   android:layout_width="270dp"
                   android:layout_height="wrap_content"
                   android:background="@drawable/vienvuong"
                   android:layout_marginLeft="10dp"/>
           </TableRow>
           <TableRow
               android:layout_marginTop="10dp">
               <TextView
                   android:layout_width="wrap_content"
                   android:layout_height="wrap_content"
                   android:text="Name"
                   android:textSize="17dp"
                   android:layout_marginLeft="10dp"/>
               <TextView
                   android:id="@+id/vName"
                   android:layout_width="270dp"
                   android:layout_height="wrap_content"
                   android:background="@drawable/vienvuong"
                   android:layout_marginLeft="10dp"/>
           </TableRow>
           <TableRow
               android:layout_marginTop="10dp">
               <TextView
                   android:layout_width="wrap_content"
                   android:layout_height="wrap_content"
                   android:text="Id"
                   android:textSize="17dp"
                   android:layout_marginLeft="10dp"/>
               <TextView
                   android:id="@+id/vId"
                   android:layout_width="270dp"
                   android:layout_height="wrap_content"
                   android:background="@drawable/vienvuong"
                   android:layout_marginLeft="10dp"/>
           </TableRow>
           <TableRow
               android:layout_marginTop="10dp">
               <TextView
                   android:layout_width="wrap_content"
                   android:layout_height="wrap_content"
                   android:text="Base"
                   android:textSize="17dp"
                   android:layout_marginLeft="10dp"/>
               <TextView
                   android:id="@+id/vcustomerbase"
                   android:layout_width="270dp"
                   android:layout_height="wrap_content"
                   android:background="@drawable/vienvuong"
                   android:layout_marginLeft="10dp"/>
           </TableRow>
           <TableRow
               android:layout_marginTop="10dp">
               <TextView
                   android:layout_width="wrap_content"
                   android:layout_height="wrap_content"
                   android:text="Need"
                   android:textSize="17dp"
                   android:layout_marginLeft="10dp"/>
               <TextView
                   android:id="@+id/vNeed"
                   android:layout_width="270dp"
                   android:layout_height="wrap_content"
                   android:background="@drawable/vienvuong"
                   android:layout_marginLeft="10dp"/>
           </TableRow>
           <TableRow
               android:layout_marginTop="10dp">
               <TextView
                   android:layout_width="wrap_content"
                   android:layout_height="wrap_content"
                   android:text="Other"
                   android:textSize="17dp"
                   android:layout_marginLeft="10dp"/>
               <TextView
                   android:id="@+id/vOther"
                   android:layout_width="270dp"
                   android:layout_height="wrap_content"
                   android:background="@drawable/vienvuong"
                   android:layout_marginLeft="10dp"/>
           </TableRow>
           <TableRow
               android:layout_marginTop="10dp">
               <TextView
                   android:layout_width="wrap_content"
                   android:layout_height="wrap_content"
                   android:text="Phone"
                   android:textSize="17dp"
                   android:layout_marginLeft="10dp"/>
               <TextView
                   android:id="@+id/vPhone"
                   android:layout_width="270dp"
                   android:layout_height="wrap_content"
                   android:background="@drawable/vienvuong"
                   android:layout_marginLeft="10dp"/>
           </TableRow>

       </TableLayout>


   </ScrollView>




</RelativeLayout>